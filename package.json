{
  "name": "@spatie/wires",
  "version": "0.4.9",
  "main": "src/index.js",
  "license": "MIT",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "next-plugin-transpile-modules": "^2.0.0",
    "nprogress": "^0.2.0",
    "webpack": "*"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.7",
    "cssnano": "^4.1.10",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "postcss-cli": "^6.1.1",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^1.16.3",
    "tailwindcss": "^1.0.0-beta"
  },
  "peerDependencies": {
    "next": "*",
    "react": "*",
    "react-dom": "*"
  },
  "scripts": {
    "prepublish": "$npm_execpath run styles",
    "styles": "postcss src/styles/index.css -o styles.css",
    "format": "prettier \"**/*.{css,js}\" --write --ignore-path .gitignore"
  },
  "files": [
    "src",
    "next.js",
    "styles.css"
  ],
  "lint-staged": {
    "linters": {
      "**/*.{css,js}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
