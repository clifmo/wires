{
  "name": "@spatie/wires",
  "version": "0.5.0-beta.5",
  "main": "src/index.js",
  "license": "MIT",
  "dependencies": {
    "next-transpile-modules": "^3.0.0",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.3",
    "postcss-cli": "^6.1.1",
    "prettier": "^1.16.3",
    "tailwindcss": "1.2.0-canary.5"
  },
  "peerDependencies": {
    "next": "^9.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "prepublish": "$npm_execpath run styles",
    "styles": "postcss src/styles/index.css -o styles.css",
    "format": "prettier \"**/*.{css,js}\" --write --ignore-path .gitignore"
  },
  "files": [
    "src",
    "next.config.js",
    "styles.css"
  ],
  "lint-staged": {
    "**/*.{css,js}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
